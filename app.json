{
  "expo": {
    "name": "Nutritrack",
    "slug": "nutritrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nutritrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#22C55E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nutritrack.app",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Nutritrack needs camera access to take photos of your meals for nutrition analysis.",
        "NSPhotoLibraryUsageDescription": "Nutritrack needs photo library access to select meal photos for nutrition analysis.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "nutritrack",
              "com.nutritrack.app",
              "com.googleusercontent.apps.269611860485-u65boej0tfss15bkiohna4o9msh3c5ch"
            ]
          }
        ]
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#22C55E"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.nutritrack.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Nutritrack accesses your photos to let you select meal images for nutrition analysis.",
          "cameraPermission": "Nutritrack accesses your camera to take photos of your meals for nutrition analysis."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#22C55E"
        }
      ],
      "expo-secure-store",
      "expo-sqlite",
      "expo-localization",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "766f4cfc-7488-487f-8dba-5c6977f55df1"
      }
    },
    "owner": "hillmanchan709",
    "updates": {
      "url": "https://u.expo.dev/766f4cfc-7488-487f-8dba-5c6977f55df1"
    }
  }
}
